{
  "domain": "cast",
  "name": "Google Cast",
  "config_flow": true,
  "documentation": "https://www.home-assistant.io/integrations/cast",
  "requirements": ["pychromecast>=12.1.4"],
  "after_dependencies": [
    "cloud",
    "http",
    "media_source",
    "plex",
    "tts",
    "zeroconf"
  ],
  "zeroconf": ["_googlecast._tcp.local."],
  "codeowners": ["@nixe64"],
  "iot_class": "local_polling",
  "loggers": ["casttube", "pychromecast"]
}
